{
  "name": "jukebox",
  "version": "1.0.0",
  "description": "Jukebox is a mobile first, digital platform that allows you to search, find and share high quality audio and video content from the biggest names in music, entertainment and pop culture.",
  "main": "index.js",
  "scripts": {
    "start": "cordova run browser",
    "watch": "gulp",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o www/css/build.css ] -e sources/index.js | uglifyjs -c warnings=false -m > www/js/index.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hercules08/jukebox.git"
  },
  "author": "Swyft Studios",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/hercules08/jukebox/issues"
  },
  "homepage": "https://github.com/hercules08/jukebox#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "framework7": "^1.6.0",
    "framework7-vue": "^0.9.0",
    "lodash": "^4.17.4",
    "vue": "^2.2.6"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "cross-env": "^4.0.0",
    "envify": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-envify": "^1.0.0",
    "gulp-gzip": "^1.4.0",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^2.1.2",
    "vinyl-source-stream": "^1.1.0",
    "vueify": "^9.4.1"
  }
}
